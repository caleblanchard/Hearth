{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "ignorePatterns": ["app/generated/**/*", "node_modules/**/*", ".next/**/*"],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "@typescript-eslint/no-explicit-any": "warn",
    "react/no-unescaped-entities": "off"
  }
}
